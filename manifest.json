{
  "manifest_version": 3,
  "name": "Zoom Tab Cleaner",
  "version": "1.0",
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },  
  "background": {
    "service_worker": "background.js"
  }
}
